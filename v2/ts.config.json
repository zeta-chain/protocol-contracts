{
    "compilerOptions": {
      "baseUrl": ".",
      "declaration": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "noImplicitAny": true,
      "outDir": "dist",
      "paths": {
      },
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "sourceMap": false,
      "strict": true,
      "target": "es2020",
      "typeRoots": ["@types", "./node_modules/@types"]
    },
    "exclude": []
  }
  