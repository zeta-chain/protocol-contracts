    "@openzeppelin/hardhat-upgrades": "1.28.0",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.3.1",
    "@types/inquirer": "^8.2.1",
    "@types/lodash": "^4.17.6",
    "@types/mocha": "^10.0.1",
    "@types/node": "^17.0.25",
