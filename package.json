    "hardhat": "^2.17.2",
    "hardhat-abi-exporter": "^2.10.1",
    "hardhat-gas-reporter": "^1.0.9",
    "inquirer": "^8.2.4",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "solhint": "^5.0.1",
    "solidity-coverage": "^0.8.12",
    "ts-mocha": "^10.0.0",
    "ts-node": "10.8.1",
    "tsconfig-paths": "^3.14.1",
    "typechain": "^8.1.0",
    "typescript": "^4.6.3",
